@article{corona2024,
title = {The Geomagnetic Stations Network of Mexico (REGMEX): The initial steps towards a real-time monitoring of geomagnetic activity in Mexico},
journal = {Journal of Atmospheric and Solar-Terrestrial Physics},
volume = {256},
pages = {106204},
year = {2024},
issn = {1364-6826},
doi = {https://doi.org/10.1016/j.jastp.2024.106204},
url = {https://www.sciencedirect.com/science/article/pii/S1364682624000324},
author = {P. Corona-Romero and J.A. Gonzalez-Esparza and P. Villanueva-Hernandez and E. Andrade-Mascote and C.I. Castellanos-Velazco and A.R. Espinosa-Jimenez and M.A. Sergeeva},
keywords = {Space Weather, Regional space climate, Geomagnetic indices, Geomagnetic storms, Geomagnetism},
abstract = {In this paper we present our first steps towards the monitoring, in near real-time, of the geomagnetic activity in the continental territory of Mexico by the Space Weather National Laboratory (LANCE) via the Geomagnetic Stations Network of Mexico (REGMEX). REGMEX aims to monitor the geomagnetic field at five different sites in real-time. The collected data will be processed to promptly calculate and publish Mexico’s regional geomagnetic-activity indices (ΔH and K). Currently, Mexico lacks facilities to simultaneously monitor geomagnetic activity in real-time across its territory. This work shows the way in which LANCE endeavors to solve the lack of such geomagnetic data by presenting the REGMEX’s development plan and its current development status. In addition, we show geomagnetic storms as registered by our available regional geomagnetic indices and compare those records with their planetary counterparts.}
}

